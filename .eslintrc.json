{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-console": "off",
        "indent": ["error", 4],
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    }
}
